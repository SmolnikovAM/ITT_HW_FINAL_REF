{
  "name": "itt_final_backend",
  "version": "1.0.0",
  "description": "refactoring of final project",
  "main": "app.mjs",
  "scripts": {
    "start": "node ./index.js",
    "start_es6": "node --experimental-modules ./app",
    "rebuild": "ITTALENTS_port=3001 ITTALENTS_dbForce=true ITTALENTS_insertTestValues=true ITTALENTS_db__params__logging=true node --experimental-modules ./app",
    "test": "jest",
    "dev_es6": "nodemon --experimental-modules ./app",
    "dev": "nodemon ./index.js"
  },
  "author": "Andrei Smolnikov",
  "license": "ISC",
  "prettier": {
    "singleQuote": true,
    "trailingComma": true
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.mjs$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.(m)js",
      "**/?(*.)+(spec|test).(m)js"
    ],
    "moduleFileExtensions": [
      "node",
      "mjs",
      "js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "rules": {
      "import/extensions": [
        "error",
        "always",
        {
          "js": "never",
          "jsx": "never",
          "mjs": "never"
        }
      ]
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".mjs"
          ]
        }
      }
    },
    "env": {
      "node": true,
      "jest": true
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "regenerator-runtime": "^0.12.1"
  },
  "dependencies": {
    "@koa/cors": "^2.2.2",
    "esm": "^3.0.79",
    "inversify": "^4.13.0",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "rc": "^1.2.8",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^4.38.0",
    "sqlite3": "^4.0.2"
  }
}
